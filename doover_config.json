{
    "morningstar_prostar_app": {
        "name": "morningstar_prostar_app",
        "display_name": "MorningStar ProStar App",
        "type": "DEV",
        "visibility": "PUB",
        "allow_many": true,
        "description": "An app to monitor and record solar and battery metrics via modbus from a MorningStar ProStar series solar controller.",
        "long_description": "README.md",
        "depends_on": [
            "modbus_interface"
        ],
        "owner_org_key": "36a22e32-ab81-4a39-b400-869d532a052d",
        "image_name": "ghcr.io/getdoover/morningstar-prostar-app:main",
        "container_registry_profile_key": "88953909-e9c4-4f81-9ec0-56239d4b0a2e",
        "build_args": "--platform linux/amd64,linux/arm64",
        "config_schema": {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "$id": "",
            "title": "Application Config",
            "type": "object",
            "properties": {
                "system_voltage": {
                    "enum": [
                        12,
                        24,
                        48
                    ],
                    "title": "System Voltage",
                    "x-name": "system_voltage",
                    "x-hidden": false,
                    "description": "System voltage for the solar controller",
                    "default": 24
                },
                "battery_max_(ah)": {
                    "title": "Battery Max (Ah)",
                    "x-name": "battery_max_(ah)",
                    "x-hidden": false,
                    "type": "number",
                    "description": "Max capacity of the battery"
                },
                "modbus_slave_id": {
                    "title": "Modbus Slave ID",
                    "x-name": "modbus_slave_id",
                    "x-hidden": false,
                    "type": "integer",
                    "description": "Modbus Slave ID for Prostar"
                },
                "modbus_config": {
                    "title": "Modbus Config",
                    "x-name": "modbus_config",
                    "x-hidden": false,
                    "type": "object",
                    "properties": {
                        "bus_type": {
                            "enum": [
                                "serial",
                                "tcp"
                            ],
                            "title": "Bus Type",
                            "x-name": "bus_type",
                            "x-hidden": false,
                            "type": "string",
                            "default": "serial"
                        },
                        "name": {
                            "title": "Name",
                            "x-name": "name",
                            "x-hidden": false,
                            "type": "string",
                            "default": "default"
                        },
                        "serial_port": {
                            "title": "Serial Port",
                            "x-name": "serial_port",
                            "x-hidden": false,
                            "type": "string",
                            "default": "/dev/ttyS0"
                        },
                        "serial_baud": {
                            "title": "Serial Baud",
                            "x-name": "serial_baud",
                            "x-hidden": false,
                            "type": "integer",
                            "default": 9600
                        },
                        "serial_method": {
                            "enum": [
                                "rtu",
                                "ascii",
                                "socket",
                                "tls"
                            ],
                            "title": "Serial Method",
                            "x-name": "serial_method",
                            "x-hidden": false,
                            "type": "string",
                            "default": "rtu"
                        },
                        "serial_data_bits": {
                            "title": "Serial Data Bits",
                            "x-name": "serial_data_bits",
                            "x-hidden": false,
                            "type": "integer",
                            "default": 8
                        },
                        "serial_parity": {
                            "enum": [
                                "None",
                                "Even",
                                "Odd"
                            ],
                            "title": "Serial Parity",
                            "x-name": "serial_parity",
                            "x-hidden": false,
                            "type": "string",
                            "default": "None"
                        },
                        "serial_stop_bits": {
                            "title": "Serial Stop Bits",
                            "x-name": "serial_stop_bits",
                            "x-hidden": false,
                            "type": "integer",
                            "default": 1
                        },
                        "serial_timeout": {
                            "title": "Serial Timeout",
                            "x-name": "serial_timeout",
                            "x-hidden": false,
                            "type": "number",
                            "default": 0.3
                        },
                        "tcp_uri": {
                            "title": "TCP URI",
                            "x-name": "tcp_uri",
                            "x-hidden": false,
                            "type": "string",
                            "default": "127.0.0.1:5000"
                        },
                        "tcp_timeout": {
                            "title": "TCP Timeout",
                            "x-name": "tcp_timeout",
                            "x-hidden": false,
                            "type": "number",
                            "default": 2.0
                        }
                    },
                    "additionalElements": true,
                    "required": []
                }
            },
            "additionalElements": true,
            "required": [
                "battery_max_(ah)",
                "modbus_slave_id",
                "modbus_config"
            ]
        },
        "key": "de97c5e2-38b9-43f3-bd80-040d9375e9af",
        "code_repo_key": null,
        "repo_branch": "main",
        "staging_config": {}
    }
}